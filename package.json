{
  "name": "pusher-js-mock",
  "version": "0.2.0",
  "description": "Mock Pusher.js in your JavaScript tests",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.js",
    "cover": "jest test --coverage",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "precommit": "yarn lint && yarn format --fix && git add -A"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolalsvk/pusher-js-mock.git"
  },
  "keywords": [
    "pusher",
    "mock",
    "test"
  ],
  "author": "Nikola ƒêuza <nikolaseap@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nikolalsvk/pusher-js-mock/issues"
  },
  "homepage": "https://github.com/nikolalsvk/pusher-js-mock#readme",
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "husky": "^0.14.3",
    "jest": "^24.9.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.0.0",
    "prettier": "^1.19.1",
    "pusher-js": "^4.2.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=4.2.4"
  },
  "files": ["lib/**/*"]
}
